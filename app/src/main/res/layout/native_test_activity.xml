<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

    <TextView
            android:id="@+id/sample_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Hello World!" />

    <Button
            android:id="@+id/btn_local_load"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="本地加载HOOK模块" />

    <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/btn_dyn_local_load"
            android:text="动态跨命名空间加载" />

    <Button
            android:id="@+id/btn_test_maps"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="测试maps屏蔽效果" />

    <Button
            android:id="@+id/btn_test_classloader"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="测试类加载" />

    <Button
            android:id="@+id/btn_test_global"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="测试Debug检测" />

    <Button
            android:id="@+id/btn_test_getenv"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="测试环境变量" />

    <Button
            android:id="@+id/btn_test_properties"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="测试SystemProperties属性" />

    <Button
            android:id="@+id/btn_test_java_relink"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="测试Java调用重定位库" />

    <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/btn_test_runtime_exec"
            android:text="测试Runtime.exec执行" />
    <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/btn_test_11_load"
            android:text="测试Android11以上加载" />
    <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/btn_test_register"
            android:text="测试重注册Native" />
    <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/btn_test_register1"
            android:text="测试重注册调用" />
</LinearLayout>